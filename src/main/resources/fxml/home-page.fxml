<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="505.0" styleClass="v_box_center" stylesheets="@../styles/common.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.oop.controller.HomePageController">

   <Label alignment="TOP_LEFT" prefHeight="59.0" prefWidth="517.0" styleClass="h1" text="Hệ Thống thu thập thông tin NFT">
      <font>
         <Font name="Berlin Sans FB Demi Bold" size="24.0" />
      </font>
   </Label>
   <VBox prefHeight="200.0" prefWidth="100.0" styleClass="v_box_center">
      <children>
      
      
         <Button fx:id="buttonTwitter" onAction="#onTwitterButtonClick" prefHeight="25.0" text="Thu thập từ twitter" />
         <Button fx:id="buttonBlogs" onAction="#onBlogButtonClick" prefHeight="25.0" text="Thu thập từ Blog" />
         <Button fx:id="buttonBinanceNFT" onAction="#onBinanceNFTButtonClick" prefHeight="25.0" text="Thu thập từ Binance NFT" />
      </children>
   </VBox>
</VBox>
